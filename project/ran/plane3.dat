  the film commands:

##--------------------------------------------------------------------------

#f setview 0,0,-80
#f setlight 0,800,-200


##---------------------------------------------------------------------------

  key traps

##---------------------------------------------------------------------------

#f trap q end
#f trap 8 up
#f trap 2 down
#f trap 6 right
#f trap 4 left
#f trap g w_invert
#f trap + speed_+
#f trap - speed_-
#f trap 7 light_l
#f trap 9 light_r
#f trap z zoom
#f trap 1 roll_l
#f trap 3 roll_r
#f trap m mac_save
#f trap n mac_load
#f trap s load_1
#f trap l load_2
#f trap o load_3


##---------------------------------------------------------------------------

   variables

##---------------------------------------------------------------------------

#f var slow_inst 2

#f var w_actv  0
#f var w_out   15
#f var w_dir   0

#f var p_dir   0
#f var p_rot   0
#f var p_speed 0

#f var alt2    0
#f var alt     0

#f var max_zoom 1

#f label loop

#f flippage
#f sky Xc3
#f ground X86
#f viewport 90,32,316,138
#f show sky_sun
#f show ground
#f show plane
#f back Xf1
#f viewport 170,147,196,173
#f show inst_alt

#f dotrap
#f label end_ops

#f jnzero w_actv w_active
#f label end_wheel

#f cp_var p_dir 0
#f bneq p_ud
#f label end_ud

#f cp_var p_rot 0
#f bneq p_rot
#f label end_rot

#f goto gnd_move
#f label end_gnd

#f jzero slow_inst loop
#f sky Xfb
#f ground Xf6
#f viewport 130,147,156,173
#f show inst_aoa
#f viewport 90,147,116,173
#f show inst_roll
#f back Xf1
#f viewport 210,147,236,173
#f show inst_speed
#f viewport 250,147,276,173
#f show inst_gear

#f viewport 290,147,316,173
#f show inst_hdg

#f add slow_inst -1
#f goto loop

#f label end
#f end


#f label w_active
#f jzero w_dir close_wheel
#f add w_out 1
#f attrib plane/lt_wheel	0,0,9,0,0,0,1
#f attrib plane/rt_wheel	0,0,-9,0,0,0,1
#f attrib plane/ft_wheel	13,0,0,0,0,0,1
#f attrib inst_gear	0,0,9,0,0,0,1
#f load slow_inst 2
#f cp_var w_out 15
#f bneq end_wheel
#f load w_actv 0
#f load w_dir 0
#f goto end_wheel

#f label close_wheel
#f add w_out -1
#f attrib plane/lt_wheel	0,0,-9,0,0,0,1
#f attrib plane/rt_wheel	0,0,9,0,0,0,1
#f attrib plane/ft_wheel	-13,0,0,0,0,0,1
#f attrib inst_gear	0,0,-9,0,0,0,1
#f load slow_inst 2
#f jnzero w_out end_wheel
#f load w_actv 0
#f load w_dir  1
#f hide  plane/lt_wheel
#f hide  plane/rt_wheel
#f hide  plane/ft_wheel
#f goto end_wheel

#f label gnd_move
#f cycle_xz ground 0,8000,4000

#f op_init
#f op_off_z p_speed
#f op ground

#f goto end_gnd

#f label p_ud
#f cp_var alt -8000
#f bless p_d
#f cp_var alt 0
#f bless ok_ud
#f cp_var p_dir 0
#f bless ok_ud
#f goto end_ud
#f label p_d
#f cp_var p_dir 0
#f bless end_ud


#f label ok_ud
#f op_init
#f op_off_y p_dir
#f op ground
#f op_init
#f op_deg_z p_dir
#f op inst_alt/alt1
#f add_var alt2 p_dir
#f add_var alt p_dir
#f cp_var alt2 -512
#f bbig less512
#f add alt2 512
#f attrib inst_alt/alt2 0,0,-64,0,0,0,1
#f goto end_ud
#f label less512
#f cp_var alt2 0
#f bless end_ud
#f add alt2 -512
#f attrib inst_alt/alt2 0,0,64,0,0,0,1
#f goto end_ud

#f label p_d
#f cp_var p_dir 0
#f bless end_ud

#f label up
#f add p_dir 10
#f attrib plane 5,0,0,0,0,0,1
#f attrib inst_aoa 0,0,5,0,0,0,1
#f load slow_inst 2
#f goto end_ops

#f label down
#f add p_dir -10
#f attrib plane -5,0,0,0,0,0,1
#f attrib inst_aoa 0,0,-5,0,0,0,1
#f load slow_inst 2
#f goto end_ops

#f label speed_+
#f add p_speed -10
#f attrib inst_speed 0,0,-8,0,0,0,1
#f load slow_inst 2
#f goto end_ops

#f label speed_-
#f add p_speed 10
#f attrib inst_speed 0,0,8,0,0,0,1
#f load slow_inst 2
#f goto end_ops


#f label p_rot
#f op_init
#f op_deg_y p_rot
#f op ground
#f op_init
#f op_deg_z p_rot
#f op inst_hdg
#f load slow_inst 2
#f goto end_rot

#f label left
#f add p_rot -1
#f attrib plane 0,0,-4,0,0,0,1
#f attrib inst_roll 0,0,4,0,0,0,1
#f load slow_inst 2
#f goto end_ops

#f label right
#f add p_rot 1
#f attrib plane 0,0,4,0,0,0,1
#f attrib inst_roll 0,0,-4,0,0,0,1
#f load slow_inst 2
#f goto end_ops

#f label roll_r
#f attrib plane 0,0,4,0,0,0,1
#f attrib inst_roll 0,0,-4,0,0,0,1
#f load slow_inst 2
#f goto end_ops

#f label roll_l
#f attrib plane 0,0,-4,0,0,0,1
#f attrib inst_roll 0,0,4,0,0,0,1
#f load slow_inst 2
#f goto end_ops

#f label w_invert
#f load w_actv 1
#f expose  plane/lt_wheel
#f expose  plane/rt_wheel
#f expose  plane/ft_wheel
#f goto end_ops

#f label light_l
#f lightmove -100,0,0
#f attrib sky_sun 0,0,0,-10,0,0,1
#f goto end_ops

#f label light_r
#f lightmove 100,0,0
#f attrib sky_sun 0,0,0,10,0,0,1
#f goto end_ops

#f label zoom
#f cp_var max_zoom 0
#f beq zoom_-
#f add max_zoom -1
#f attrib plane 0,0,0,0,0,0,2
#f goto end_ops
#f label zoom_-
#f cp_var max_zoom 1
#f beq end_ops
#f add max_zoom 1
#f attrib plane 0,0,0,0,0,0,0.5
#f goto end_ops

#f label mac_save
#f macrodump plane.mac
#f goto end_ops

#f label mac_load
#f macroload plane.mac
#f macrorun
#f goto end_ops

#f label load_1
#f macroload light.mac
#f macrorun
#f goto end_ops

#f label load_2
#f macroload roll.mac
#f macrorun
#f goto end_ops

#f label load_3
#f macroload demo.mac
#f macrorun
#f goto end_ops
